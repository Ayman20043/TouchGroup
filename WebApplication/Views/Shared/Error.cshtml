@model System.Web.Mvc.HandleErrorInfo
    @{ 
        Layout = null;
    }
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Touch Group </title>
    <!-- Bootstrap core CSS -->
    <link href="~/Content/Admin/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/fonts/fonts/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/css/animate.min.css" rel="stylesheet" />
    <!-- Custom styling plus plugins -->
    <link href="~/Content/Admin/css/custom.css" rel="stylesheet" />
    <link href="~/Content/Admin/css/icheck/flat/green.css" rel="stylesheet">

    <script src="~/Scripts/Admin/js/jquery.min.js"></script>
   
</head>

<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <!-- page content -->
            <div class="col-md-12">
                <div class="col-middle">
                    <div class="text-center text-center">
                        <h1 class="error-number">404</h1>
                        <h2>Sorry but we couldnt find this page</h2>
                        <p>
                            This page you are looking for does not exsist <a href="#">Report this?</a>
                        </p>
                        <div class="mid_center">
                            <form>
                                <div class="col-xs-12 form-group pull-right top_search">
                                    <div class="input-group">

                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /page content -->
        </div>
        <!-- footer content -->
    </div>
    <div id="custom_notifications" class="custom-notifications dsp_none">
        <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group"></ul>
        <div class="clearfix"></div>
        <div id="notif-group" class="tabbed_notifications"></div>
    </div>
    <script src="~/Scripts/Admin/js/bootstrap.min.js"></script>
    <script src="~/Scripts/Admin/js/progressbar/bootstrap-progressbar.min.js"></script>
    <script src="~/Scripts/Admin/js/nicescroll/jquery.nicescroll.min.js"></script>
    <!-- icheck -->
    <script src="~/Scripts/Admin/js/icheck/icheck.min.js"></script>
    <script src="~/Scripts/Admin/js/custom.js"></script>
    <script src="~/Scripts/Admin/js/pace/pace.min.js"></script>
</body>
</html>

