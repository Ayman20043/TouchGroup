@model IEnumerable<WebApplication.Models.CompanyProfile>

<div id="ProfilePartial">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SectionName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Content)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SectionName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Content)
                </td>
                <td>
                    <a id="ViewClick" data-id="@item.Id" class="btn btn-primary btn-xs clickview"><i class="fa fa-folder"></i> View </a>
                    <a id="EditeClick" data-id="@item.Id" class="btn btn-info btn-xs edite clickedit"><i class="fa fa-pencil "></i> Edit </a>
                    <a id="DeleteClick" data-id="@item.Id" class="btn btn-danger btn-xs clickdelete"><i class="fa fa-trash-o"></i> Delete </a>
                </td>
            </tr>
        }

    </table>
</div>